<%- include('./includes/navbar')%>
<div class="container">
<div class="rounded-3 opacity-25 modal modal-signin position-static d-block bg-light py-5" tabindex="-1" role="dialog" id="modalSignin">
  <div class="modal-dialog" role="document">
    <div class="modal-content rounded-4 shadow">
      <div class="modal-header p-5 border-bottom-0">
        <h1 class="fw-bold mb-0 fs-2">Stwórz Przedmiot</h1>
      </div>
      <div class="modal-body p-5 pt-0">
        <form action=<%= data %>  method=<%= method %>>
          <div class="form-floating mb-3">
            <input type="text" class="form-control rounded-3" name="name" id="name" placeholder="Matematyka I" required>
            <label for="floatingInput">Nazwa Przedmiotu</label>
          </div>
          <button class="w-100 mb-2 btn btn-lg rounded-3 btn-primary mt-4" type="submit">Stwórz</button>
          <small class="text-muted">By clicking Sign up, you agree to the terms of use.</small>
        </form>
      </div>
    </div>
  </div>
</div>
</div>
<%- include('./includes/footer') %></div>